{
    "presets": [
        "env"
    ],
    "plugins": [
        "transform-runtime",
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": "css"
            }
        ],
        [
            "module-resolver",
            {
                "root": [
                    "./client"
                ],
                "alias": {
                    "components": "./client/components"
                }
            }
        ]
    ]
}