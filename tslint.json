{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "no-console": false,
        "max-line-length": false,
        "max-classes-per-file": false,
        "no-var-requires": false,
        "curly": false,
        "no-unused-expression": false,
        "member-ordering": false,
        "variable-name": false,
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ]
    },
    "rulesDirectory": []
}